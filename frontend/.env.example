# Frontend Environment Configuration
# Copy this file to .env and fill in your actual values
# Do NOT commit the actual .env file with sensitive information

# API URLs
# In Docker, use the backend service name and internal port
REACT_APP_API_URL=http://backend:6102/api
# For local development:
# REACT_APP_API_URL=http://localhost:6102/api

# LLM Engine URL (if needed directly)
REACT_APP_LLM_API_URL=http://llm-engine:6101/api
# For local development:
# REACT_APP_LLM_API_URL=http://localhost:6101/api

# Development Server
PORT=8081
HOST=0.0.0.0

# Feature Flags
REACT_APP_ENABLE_FEEDBACK=true
REACT_APP_ENABLE_FILE_UPLOAD=false

# Misc
NODE_ENV=production
